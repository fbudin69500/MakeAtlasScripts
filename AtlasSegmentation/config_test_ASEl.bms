##If not, copy and modify this example to have a version of the configuration file that is up
##to date.
##Version:1.1
#####################################################################################################
################################### Variables to set ################################################
#####################################################################################################
##-1-Name of this file (configuration file).
##Set to '' if you don't want to save the configuration file in the output directory
set( ScriptName config_test_AS.bms )
##The files must have the following structure:
##dir/case/case_suffix;dir=/rodent/Project/subProject1
##-2-SubProject root directory
set( dir /rodent/MIDAS/Data )
##-5-Atlas subdirectory for brain stripping and parcellation
set( ATLAS_SUB_DIR Atlas-PN45Tests/template )
##-6-Population atlas directories suffix
set( AtlasDirectoriesSuffix PN45Tests )
##-6b-Population atlas directories suffix OUTPUT
set( AtlasDirectoriesSuffixOut PN45TestsMDElastic )
##Population atlas file name
set( PopulationAtlas AverageTensor_SS_MD_sp.nrrd )
##-7-Select which kind of registration is gonna be used. FALSE = Elastic, TRUE = Diffeomorphic
set( Diffeomorphic FALSE )
###################################
#Extra parameters. Normally no need to modified
###################################
##-10-Input pre-subdir. For old data structure, the value should be "."
set( INPUT_PRE_SUBDIR . )
##-11-Output pre-subdir. For old data structure, the value should be "."
set( OUTPUT_PRE_SUBDIR . )
##-12-Directory in which the computed population atlas and its parcellation/segmentation are saved
##-12-Directory in which the temp directory is written and the final output is written
set( outputDir ${dir}/${OUTPUT_PRE_SUBDIR}/Atlas-${AtlasDirectoriesSuffixOut} )
##-13-Atlas directory for brain stripping and parcellation
set( ATLAS_DIR /rodent/atlas/MouseImagingCenter )
##-14-Directory in which the computed population atlas and its parcellation/segmentation are saved (Average_ImageSS)
set( PopAtlasDir ${dir}/${OUTPUT_PRE_SUBDIR}/Atlas-${AtlasDirectoriesSuffix} )
##-16-Directory in which temporary files are written
set( atlasProcessDir ${outputDir}/temp-MarionTestingScripts )
##-18-If the transform given above was computed with image spacing set to 1,1,1 and centered, set to TRUE (Not used for scalar images)
set( IS_SCALED FALSE )
##-22-Atlas file
set( TEMPLATE male-female-mouse-atlas.nii.gz )
##-23-Name of the parcellation atlas
set( PARCELLATION_FILE_NAME c57_fixed_labels_resized.nii.gz )
##-24-Image grid filename
set( IMAGE_GRID ImageGrid.nrrd )
##-26- Similarity metrics that is gonna be used with ANTS. Can be MI, MSQ, CC, PR, PSE (please refer to the doc)
set (Measure MI)
##-27- ANTS ranges
set (weight 1 )
set (bin_number 32 )
##-28- ANTS if set to 0, no deformable mapping is performed.
set (defIterations 0 )
##-29- ANTS image dimension
set (imgDimension 3 )
##-30-Step at which to start the processing (only use if script stopped in the middle of the execution for no real reason)
set (step 0)
###################################
include( /home/hoogstoe/Scripts/local-scripts/MakeAtlasScripts/GlobalConfig.bms )
##Processes
include( /home/hoogstoe/Scripts/local-scripts-Marion-ANTS/MakeAtlasScripts/AtlasSegmentation/000AtlasSegmentation.bms )
